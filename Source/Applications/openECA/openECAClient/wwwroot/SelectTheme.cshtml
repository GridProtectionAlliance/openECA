@using System.Collections.Specialized
@using System.Linq
@using System.Net.Http
@using GSF
@using GSF.Configuration
@using GSF.IO
@using GSF.Web
@using GSF.Web.Hosting
@using openECAClient
@using openECAClient.Model
@using RazorEngine.Templating
@inherits TemplateBase<AppModel>
@{
    Layout = "Layout.cshtml";
    ViewBag.Title = "Select Theme";

    HttpRequestMessage request = ViewBag.Request;
    Dictionary<string, string> parameters = request.QueryParameters();

    string selectedTheme;
    parameters.TryGetValue("selectedTheme", out selectedTheme);

    string currentTheme = FilePath.GetFileName(Model.Global.BootstrapTheme).Trim();
    string[] fileList = FilePath.GetFileList(string.Format("{0}Content/bootstrap-theme-*.css", FilePath.AddPathSuffix(WebServer.Default.WebRootPath))).Select(fileName => FilePath.GetFileName(fileName).Trim()).ToArray();

    if (selectedTheme == null) {
        selectedTheme = currentTheme;
    }
    else {
        CategorizedSettingsElementCollection systemSettings = ConfigurationFile.Current.Settings["systemSettings"];
        systemSettings["BootstrapTheme"].Value = "Content/" + selectedTheme;
        Model.Global.BootstrapTheme = systemSettings["BootstrapTheme"].Value;
        ConfigurationFile.Current.Save();
    }
}
<div class="well">
    <form role="form">
        <div class="form-group">
            <label for="selectBootstrapTheme">Selected Bootstrap Theme:</label>
            <select class="form-control" id="selectBootstrapTheme" name="selectedTheme">
                <option value="bootstrap-theme.css"@(string.Compare(selectedTheme, "bootstrap-theme.css", StringComparison.OrdinalIgnoreCase) == 0 ? " selected" : "")>Default Bootstrap Theme</option>
                @foreach (string theme in fileList) {
                    <option value="@theme"@(string.Compare(selectedTheme, theme, StringComparison.OrdinalIgnoreCase) == 0 ? " selected" : "")>@(FilePath.GetFileNameWithoutExtension(theme).Substring(16).ToTitleCase())</option>
                }
            </select>
            <br />
            <button class="btn btn-primary pull-right" onclick="$.post('@ViewBag.Request.RequestUri.AbsolutePath', { 'selectedTheme': $('#selectBootstrapTheme option:selected').text() });">Apply</button>
            <br />
        </div>
    </form>
</div>
